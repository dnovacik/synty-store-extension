{
  "name": "Panaxeo - Jira Scrum Update Task Filler",
  "description": "Create your daily scrum update task string formatted",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "activeTab",
    "clipboardRead",
    "clipboardWrite"
  ],
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://jira.medrio.com/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "browser_action": {}
}